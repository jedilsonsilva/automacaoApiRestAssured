{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "compeCode": {
          "type": "string"
        },
        "branchCode": {
          "type": "string"
        },
        "number": {
          "type": "string"
        },
        "checkDigit": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "subtype": {
          "type": "string"
        },
        "currency": {
          "type": "string"
        }
      },
      "required": [
        "compeCode",
        "branchCode",
        "number",
        "checkDigit",
        "type",
        "subtype",
        "currency"
      ]
    },
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        },
        "first": {
          "type": ["string", "null"]
        },
        "prev": {
          "type": ["string", "null"]
        },
        "next": {
          "type": ["string", "null"]
        },
        "last": {
          "type": ["string", "null"]
        }
      },
      "required": [
        "self",
        "first",
        "prev",
        "next",
        "last"
      ]
    },
    "meta": {
      "type": "object",
      "properties": {
        "totalRecords": {
          "type": "integer"
        },
        "totalPages": {
          "type": "integer"
        },
        "requestDateTime": {
          "type": "string"
        }
      },
      "required": [
        "totalRecords",
        "totalPages",
        "requestDateTime"
      ]
    }
  },
  "required": [
    "data",
    "links",
    "meta"
  ]
}